<h1>Profile</h1>
<p><%= @user.name %> (<%= @user.email %>)</p>
<h2>Events created:</h2>
<% @user.events.each do |e| %>
<li><%= link_to e.name, e %></li>
<% end %>
<h2>Previously attended events:</h2>
<ul>
<% @prev_events.each do |e| %>
<li><%= link_to e.name, e %></li>
<% end %>
</ul>
<h2>Upcoming events:</h2>
<ul>
<% @upcoming_events.each do |e| %>
<li><%= link_to e.name, e %></li>
<% end %>
</ul>